      <ol id="active-issues-user-<%= h user.id -%>" class="active-issues <%= allowed_to_assign_staffed_issue_to(user) -%>">
        <% if @kanban.active_issues[user].empty? %>
        <%= render :partial => 'empty_issue' %>
        <% else %>
        <%= render(:partial => 'issue',
                   :collection => @kanban.active_issues[user].collect(&:issue),
                   :locals => { :limit => Setting['plugin_redmine_kanban']["panes"]["active"]["limit"].to_i }) %>
        <% end %>
      </ol>

